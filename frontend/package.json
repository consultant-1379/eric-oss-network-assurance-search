{
  "name": "eric-oss-network-assurance-search-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "test:install": "playwright install firefox chromium webkit",
    "test": "web-test-runner --playwright --browsers firefox chromium",
    "test:all": "web-test-runner --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner --watch --playwright --browsers chromium",
    "test:api": "mocha --recursive",
    "test:contract": "web-test-runner test/contract/**/*.contract.js --playwright --browsers chromium --config web-test-runner.contract.config.js",
    "test:playwright": "npx playwright test",
    "test:playwright:ui": "npx playwright test --ui",
    "start": "snowpack dev --config snowpack.config.dev.js",
    "build:dev": "snowpack build",
    "build:dev:watch": "nodemon --watch src --exec snowpack build",
    "build:prod": "snowpack build && rollup -c",
    "srv": "node server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .eslintignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .eslintignore",
    "lint:prettier": "prettier \"**/*.{js,html}\" --check --ignore-path .eslintignore",
    "format:prettier": "prettier \"**/*.{js,html}\" --write --ignore-path .eslintignore",
    "e2e:local": "wdio test_js/config/wdio.conf.cjs --selenium-standalone --local-mockserver --spec-reporter",
    "e2e:local-docker": "wdio test_js/config/wdio.conf.cjs --selenium-hub --local-mockserver --spec-reporter",
    "e2e:CI": "wdio test_js/config/wdio.conf.cjs --ci --selenium-hub --network-config-from-env  --spec-reporter --allure-reporter",
    "e2e:generateReport": "node scripts/test_js-generate-report.js",
    "openReport": "allure open test_js/allure-report",
    "sonar-scanner": "sonar-scanner"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adp/ui-common": "0.1.11",
    "@eui/app": "1.0.0",
    "@eui/base": "^1.8.3",
    "@eui/component": "^1.0.4",
    "@eui/container": "^1.2.3",
    "@eui/layout": "^1.3.1",
    "@eui/lit-component": "^1.2.0",
    "@eui/table": "1.3.9",
    "@eui/theme": "^1.2.3",
    "lit-html": "3.1.1",
    "@webcomponents/scoped-custom-element-registry": "0.0.9",
    "dayjs": "1.11.7",
    "es-module-shims": "1.8.2",
    "jsonpath-plus": "4.0.0",
    "jsonschema": "1.4.1",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "luxon": "3.2.1",
    "qs": "6.11.0",
    "superagent": "8.0.0",
    "urlpattern-polyfill": "8.0.2"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "8.0.2",
    "@open-wc/testing": "^3.1.6",
    "@open-wc/testing-helpers": "^2.1.3",
    "@playwright/test": "^1.39.0",
    "@wdio/allure-reporter": "^7.16.13",
    "@wdio/cli": "^7.16.8",
    "@wdio/local-runner": "^7.16.8",
    "@wdio/mocha-framework": "^7.16.6",
    "@wdio/selenium-standalone-service": "^7.16.6",
    "@wdio/spec-reporter": "^7.16.4",
    "@web/dev-server-import-maps": "^0.0.7",
    "@web/test-runner": "^0.14.0",
    "@web/test-runner-junit-reporter": "0.4.11",
    "@web/test-runner-playwright": "^0.8.10",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "allure-commandline": "^2.17.2",
    "chai": "^4.3.6",
    "chai-sorted": "^0.2.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-lit-a11y": "2.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "sinon": "14.0.0",
    "sonarqube-scanner": "^2.8.1",
    "snowpack": "3.8.8",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@snowpack/web-test-runner-plugin": "0.2.2",
    "@babel/register": "7.0.0",
    "express": "4.19.2",
    "@eui/import-css-plugin": "1.0.0",
    "rollup": "2.79.1",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "rollup-plugin-summary": "1.4.3",
    "rollup-plugin-terser": "7.0.2",
    "glob": "8.0.3"
  },
  "overrides": {
    "semver": "7.5.4",
    "tough-cookie": "4.1.3"
  }
}
